{
    "name": "benborla/jobadder-api-client",
    "description": "JobAdder API Client",
    "type": "library",
    "require": {
        "php": ">=8.1.0",
        "psr/log": "^3.0",
        "psr/http-message": "^1.0",
        "symfony/serializer": "^6.0.0",
        "jane-php/open-api-runtime": "^7.0",
        "roave/security-advisories": "dev-latest"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "phpunit/php-invoker": "^3.1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Borla",
            "email": "benborla@icloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BenBorla\\JobAdder\\": "src/",
            "BenBorla\\JobAdder\\V2\\": "generated/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenBorla\\JobAdder\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
